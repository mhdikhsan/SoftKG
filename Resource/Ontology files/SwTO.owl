        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasTestFundamental"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#TestFundamental"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasTestActivity"/>
        </owl:onProperty>
