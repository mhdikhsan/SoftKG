        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1964_Smcxpcfqne_DE"/>
        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#438_Smcxpcfqne"/>
