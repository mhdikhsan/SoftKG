    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_429_Xwjztikxpq_546 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_429_Xwjztikxpq_546">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">In connection with the DAML project for bringing about the Semantic Web, an ontology of time is being developed for describing the temporal content of Web pages and the temporal properties of Web services. This ontology covers topological properties of instants and intervals, measures of duration, and the meanings of clock and calendar terms..</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1864_Xwjztikxpq_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1869_Xwjztikxpq_DE"/>
