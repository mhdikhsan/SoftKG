        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#445_Ikzuyrplmq"/>
    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_446_Getichyjdh_579 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_446_Getichyjdh_579">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">Matchmaking is an important aspect of E-Commerce interactions. The current trend in B2B E-Commerce automation is towards complex interactions for service provision. In this context, matchmaking services require rich and exible metadata as well as matching algorithms. The Semantic Web initiative at W3C is gaining momentum and generating suitable technologies and tools to cover both the metadata and the al-.</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#2079_Getichyjdh_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#2082_Getichyjdh_DE"/>
