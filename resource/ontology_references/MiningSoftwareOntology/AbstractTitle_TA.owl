        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#439_Fycezbjcjs"/>
    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_440_Twsehjqoxn_568 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_440_Twsehjqoxn_568">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">Ontologies are at the heart of the semantic web. They define the concepts and relationships that make global interoperability possible. However, as these ontologies grow in size they become more and more difficult to create, use, understand, maintain, transform and classify. We present and evaluate several algorithms for extracting relevant segments out of large description logic ontologies for.</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1997_Twsehjqoxn_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#2000_Twsehjqoxn_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#2003_Twsehjqoxn_DE"/>
