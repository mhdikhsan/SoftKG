        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1926_Ejkjayacqs_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1935_Ejkjayacqs_DE"/>
        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#435_Ejkjayacqs"/>
    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_436_Nghleqhnht_559 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_436_Nghleqhnht_559">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">The Semantic Web realization depends on the availability of a critical mass of metadata for the web content, associated with the respective formal knowledge about the world. We claim that the Semantic Web, at its current stage of development, is in a state of a critically need of metadata generation and usage schemata that are specific, well-defined and easy to.</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1941_Nghleqhnht_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#1944_Nghleqhnht_DE"/>
