    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_528_Wdnktxzxxi_742 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_528_Wdnktxzxxi_742">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">The goal of the next generation Web is to build virtual communities, wherein software agents and people can work in cooperation by sharing knowledge. To achieve this goal, the emerging Semantic Web community has proposed ontologies to express knowledge in a machine understandable way. The process of building and maintaining ontologies, which is known as Ontology Engineering, presents unique challenges..</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3113_Wdnktxzxxi_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3116_Wdnktxzxxi_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3119_Wdnktxzxxi_DE"/>
