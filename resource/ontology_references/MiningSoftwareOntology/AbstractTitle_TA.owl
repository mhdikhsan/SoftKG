    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_521_Fdeauhffao_728 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_521_Fdeauhffao_728">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">An ontology is an explicit formal conceptualization of some domain of interest. Ontologies are increasingly used in various fields such as knowledge management, information extraction, and the semantic web. Ontology evaluation is the problem of assessing a given ontology from the point of view of a particular criterion of application, typically in order to determine which of several ontologies would.</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3010_Fdeauhffao_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3013_Fdeauhffao_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3016_Fdeauhffao_DE"/>
        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#521_Fdeauhffao"/>
    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_522_Mxmnvbnebg_730 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_522_Mxmnvbnebg_730">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">This paper discusses technology to support the use of UML for representing ontologies and domain knowledge in the Semantic Web. Two mappings have been defined and implemented using XSLT to produce Java classes and an RDF schema from an ontology represented as a UML class diagram and encoded using XMI. A Java application can encode domain knowledge as an object.</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3021_Mxmnvbnebg_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3026_Mxmnvbnebg_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3035_Mxmnvbnebg_DE"/>
        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#522_Mxmnvbnebg"/>
    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_523_Oqflpvpatw_732 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_523_Oqflpvpatw_732">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">A new smart meeting room system called EasyMeeting explores the use of multi-agent systems, Semantic Web ontologies, reasoning, and declarative policies for security and privacy. Building on an earlier pervasive computing system, EasyMeeting provides relevant services and information to meeting participants based on their situational needs. The system also exploits the context-aware support provided by the Context Broker Architecture (Cobra)..</rdfs:comment>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3041_Oqflpvpatw_DE"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3046_Oqflpvpatw_DE"/>
