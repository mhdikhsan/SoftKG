    </owl:NamedIndividual>
    


    <!-- http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_294_Wgxczhjyyq_590 -->

    <owl:NamedIndividual rdf:about="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#frag_294_Wgxczhjyyq_590">
        <rdf:type rdf:resource="&annotDocuScientific;Fragment"/>
        <rdfs:comment rdf:datatype="&xsd;string">The increasing number of ontologies of the semantic Web poses new challenges for ontology mapping. In the context of question answering there is a need for good mapping algorithms which efficiently can perform syntactic and semantic mappings between classes and class properties from different ontologies. Mapping algorithms without the help of human experts are in particular desirable when the answer.</rdfs:comment>
        <annotDocuScientific:part_of rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#294_Wgxczhjyyq"/>
        <annotDocuScientific:has_discourse_element rdf:resource="http://cui.unige.ch/~deribauh/DocumentAnnote.owl#3713_Wgxczhjyyq_DE"/>
